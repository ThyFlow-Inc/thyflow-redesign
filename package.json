{
  "name": "reactjs-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "highlight.js": "~10.1.2",
    "jquery": "^3.4.1",
    "js-cookie": "~2.1.4",
    "mailchimp-api-v3": "^1.7.0",
    "node-sass": "^4.5.2",
    "prismic-toolbar": "0.0.1",
    "prismic.io": "~3.5.5",
    "qs": "^6.4.0",
    "react": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-router-dom": "~4.0.0",
    "react-router-page-transition": "^3.0.2",
    "react-router-transition": "^0.1.1",
    "react-transition-group": "^1.1.3",
    "sass": "^0.5.0",
    "sass-mq": "^3.3.2",
    "spectre.scss": "0.0.1"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "axios": "^0.19.0",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "6.2.10",
    "babel-preset-react-app": "^2.2.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "^3.2.0",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "eslint": "^4.18.2",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "^0.19.1",
    "jest": "^24.9.0",
    "json-loader": "0.5.4",
    "lost": "^8.0.0",
    "npm-run-all": "^4.0.2",
    "object-assign": "4.1.1",
    "postcss-at2x": "^3.2.0",
    "postcss-loader": "1.2.2",
    "postcss-pxtorem": "^4.0.0",
    "postcss-responsive-type": "^0.5.1",
    "postcss-vertical-rhythm": "^1.1.5",
    "promise": "7.1.1",
    "react-dev-utils": "^9.0.3",
    "react-inlinesvg": "^0.6.0",
    "react-timestamp": "^4.1.0",
    "sass-loader": "^6.0.3",
    "style-loader": "0.13.1",
    "svg-sprite-loader": "^2.0.1",
    "svgo": "^1.3.0",
    "svgo-loader": "^1.2.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-dev-server": "^3.8.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start-js": "node scripts/start.js",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && node scripts/build.js",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "test": "node scripts/test.js --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "engines": {
    "node": ">=4.8.1",
    "npm": ">=2.15.11"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
